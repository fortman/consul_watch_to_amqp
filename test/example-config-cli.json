{
  "storage": {
    "storage_name": "testing",
    "classname": "ConsulWatcher::Storage::Consul"
  },
  "watch_type": {
    "classname": "ConsulWatcher::WatchType::Key",
    "filters": {
      "key_property": [ "ModifyIndex" ],
      "key_path": [ ".*asd.*" ]
    }
  },
  "destination": {
    "classname": "ConsulWatcher::Destination::Amqp",
    "rabbitmq_host": "localhost"
  }
}
